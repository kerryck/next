{
    "name": "Gallerify the Web",
    "version": "1",
    "description":
        "Adds next and previous functionality to websites with numerical URLs.",
    "page_action": {
        "default_icon": "next-icon.png",
        "default_title": "Next"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background_page": "background.html",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
